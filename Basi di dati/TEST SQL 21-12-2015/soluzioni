1)

select persona.cf,persona.annonascita,persona.nazionenascita,nazione.continente
from persona join viaggio on persona.cf = viaggio.persona join nazione on viaggio.nazione = nazione.nome

2)

select persona.cf,nazione.continente
from persona,nazione,viaggio
where persona.annonascita < '1980' and persona.cf = viaggio.persona and nazione.nome = viaggio.nazione
order by persona.cf,nazione.continente

3)
select *
from persona as p1,persona as p2,nazione,viaggio as v1, viaggio as v2
where p1.cf != p2.cf and p1.cf = v1.persona and p2.cf = v2.persona and v1.anno = v2.anno and v1.nazione = nazione.nome and v2.nazione = nazione.nome

4)

select persona.cf,viaggio.nazione, viaggio.anno-persona.annonascita as anno
from persona join viaggio on persona.cf = viaggio.persona

5)

select persona.cf,nazione.continente, min(viaggio.anno)-persona.annonascita as eta
from persona join viaggio on persona.cf = viaggio.persona join nazione on viaggio.nazione = nazione.nome
where nazione.continente not in (select nazione.continente
								from nazione 
								where persona.nazionenascita = nazione.nome)

group by persona.cf,nazione.continente


6)

select nazione.continente,viaggio.anno,count(*)
from viaggio join nazione on viaggio.nazione = nazione.nome
group by nazione.continente,viaggio.anno

7)

select persona.cf
from persona
where persona.cf not in (select persona.cf
			from persona join viaggio on persona.cf = viaggio.persona
			where persona.nazionenascita = viaggio.nazione)

8)

select persona.cf
from persona join viaggio on viaggio.persona = persona.cf join nazione on nazione.nome = viaggio.nazione
where nazione.nome = all(select nazione.nome
			 from nazione 
			where nazione.continente = 'Oceania')
6)

select persona.cf, count(*)
from persona join viaggio on persona.cf = viaggio.persona join nazione on nazione.nome = viaggio.nazione
where viaggio.anno < 2000
group by persona.cf,nazione.continente





